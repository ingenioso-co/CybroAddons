<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    About Template-->
    <template id="about_page" name="About">
        <t t-call="website.layout">
            <div class="oe_structure o_editable">
                <section class="sub_banner about_banner"
                         t-att-style="'background-image: url(/theme_voltro/static/src/images/banner/about-banner.png)'">
                    <div class="wrapper">
                        <div class="container">
                            <div class="sub_banner_content">
                                <h3>About</h3>
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item">
                                            <a href="/">Home</a>
                                            <svg width="9" height="13"
                                                 viewBox="0 0 9 13" fill="none"
                                                 xmlns="http://www.w3.org/2000/svg">
                                                <path
                                                        d="M8.56628 6.99805L2.88269 12.7109C2.58972 12.9746 2.15027 12.9746 1.8866 12.7109L1.21277 12.0371C0.949097 11.7734 0.949097 11.334 1.21277 11.041L5.72449 6.5L1.21277 1.98828C0.949097 1.69531 0.949097 1.25586 1.21277 0.992188L1.8866 0.318359C2.15027 0.0546875 2.58972 0.0546875 2.88269 0.318359L8.56628 6.03125C8.82996 6.29492 8.82996 6.73438 8.56628 6.99805Z"
                                                        fill="white"/>
                                            </svg>
                                        </li>
                                        <li class="breadcrumb-item active"
                                            aria-current="page">
                                            About
                                        </li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="oe_structure o_editable">
                <section class="about_page" data-snippet="services_banner"
                         data-name="services">
                    <div class="wrapper">
                        <div class="container">
                            <div class="service">
                                <div class="service_wrapprer">
                                    <img src="/theme_voltro/static/src/images/about/Services.svg"
                                         alt=""/>
                                    <h5>FREE AND FAST DELIVERY</h5>
                                    <p>
                                        Free delivery for all orders over $140
                                    </p>
                                </div>
                                <div class="service_wrapprer">
                                    <img src="/theme_voltro/static/src/images/about/Services (3).svg"
                                         alt=""/>
                                    <h5>24/7 CUSTOMER SERVICE</h5>
                                    <p>
                                        Friendly 24/7 customer support
                                    </p>
                                </div>
                                <div class="service_wrapprer">
                                    <img src="/theme_voltro/static/src/images/about/Services (4).svg"
                                         alt=""/>
                                    <h5>MONEY BACK GUARANTEE</h5>
                                    <p>
                                        We return money within 30 days
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="oe_structure o_editable">
                <section class="about_story">
                    <div class="wrapper">
                        <div class="container">
                            <!-- Our Story -->
                            <div class="our_story">
                                <div class="row">
                                    <div class="col-lg-6 col-12">
                                        <div class="our_story_right">
                                            <h3>Our Story</h3>
                                            <p>
                                                Launched in 2015, TechMart is
                                                South
                                                Asia's leading online
                                                marketplace
                                                for
                                                gadgets, with a strong presence
                                                in
                                                Bangladesh. Offering a wide
                                                range
                                                of
                                                innovative marketing, data,
                                                and service solutions, TechMart
                                                features 10,500 sellers and 300
                                                top
                                                brands,
                                                serving 3 million tech
                                                enthusiasts
                                                across the region.
                                            </p>
                                            <p>
                                                Exclusive has more than 1
                                                Million
                                                products to offer, growing at a
                                                very
                                                fast.
                                                Exclusive offers a diverse
                                                assotment
                                                in categories ranging from
                                                consumer.
                                            </p>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-12">
                                        <div class="our_story_left">
                                            <img src="/theme_voltro/static/src/images/about/about-1.png"
                                                 alt=""/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="oe_structure o_editable">
                <!-- Achievements -->
                <section class="about_achievements">
                    <div class="wrapper">
                        <div class="container">
                            <div class="achievements">
                                <div class="stats_wrapp">
                                    <div class="stat">
                                        <img src="/theme_voltro/static/src/images/about/1.svg"
                                             alt=""/>
                                        <h2 class="number" data-target="10">0
                                        </h2>
                                        <p class="type">Sallers active our
                                            site
                                        </p>
                                    </div>
                                    <div class="stat">
                                        <img src="/theme_voltro/static/src/images/about/2.svg"
                                             alt=""/>
                                        <h2 class="number" data-target="99">0
                                        </h2>
                                        <p class="type">Monthly Produduct
                                            Sale
                                        </p>
                                    </div>
                                    <div class="stat active">
                                        <img src="/theme_voltro/static/src/images/about/3.svg"
                                             alt=""/>
                                        <h2 class="number" data-target="60">0
                                        </h2>
                                        <p class="type">Customer active in our
                                            site
                                        </p>
                                    </div>
                                    <div class="stat">
                                        <img src="/theme_voltro/static/src/images/about/Services.png"
                                             alt=""/>
                                        <h2 class="number" data-target="30">0
                                        </h2>
                                        <p class="type">Anual gross sale in our
                                            site
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="oe_structure"/>
            <!-- Team -->
            <section class="about_team">
                <div class="wrapper">
                    <div class="container">
                        <div class="team">
                            <div class="card_wrapper">
                                <div class="img_wrapper">
                                    <img src="/theme_voltro/static/src/images/about/Frame 874.png"
                                         alt=""/>
                                </div>
                                <div class="card_bottom">
                                    <div class="name">Jacob Thomas</div>
                                    <div class="position">
                                        Founder &amp; Chairman
                                    </div>
                                    <div class="icons">
                                        <img src="/theme_voltro/static/src/images/about/Icon-Twitter.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/icon-instagram.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/Icon-Linkedin.png"
                                             alt=""/>
                                    </div>
                                </div>
                            </div>
                            <div class="card_wrapper">
                                <div class="img_wrapper">
                                    <img src="/theme_voltro/static/src/images/about/Frame 874.png"
                                         alt=""/>
                                </div>
                                <div class="card_bottom">
                                    <div class="name">Jacob Thomas</div>
                                    <div class="position">
                                        Founder &amp; Chairman
                                    </div>
                                    <div class="icons">
                                        <img src="/theme_voltro/static/src/images/about/Icon-Twitter.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/icon-instagram.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/Icon-Linkedin.png"
                                             alt=""/>
                                    </div>
                                </div>
                            </div>
                            <div class="card_wrapper">
                                <div class="img_wrapper">
                                    <img src="/theme_voltro/static/src/images/about/Frame 874.png"
                                         alt=""/>
                                </div>
                                <div class="card_bottom">
                                    <div class="name">Jacob Thomas</div>
                                    <div class="position">
                                        Founder &amp; Chairman
                                    </div>
                                    <div class="icons">
                                        <img src="/theme_voltro/static/src/images/about/Icon-Twitter.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/icon-instagram.png"
                                             alt=""/>
                                        <img src="/theme_voltro/static/src/images/about/Icon-Linkedin.png"
                                             alt=""/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </t>
    </template>
</odoo>
